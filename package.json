{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@capacitor/android": "^5.2.3",
    "@capacitor/cli": "^5.2.3",
    "@capacitor/core": "^5.2.3",
    "@capacitor/ios": "^5.2.3",
    "@tanstack/vue-query": "^4.32.6",
    "@vueuse/core": "^10.3.0",
    "axios": "^1.4.0",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.2",
    "@iconify/vue": "^4.1.1",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^20.5.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.47.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.1.6",
    "unplugin-vue-router": "^0.6.4",
    "vite": "^4.4.6",
    "vue-tsc": "^1.8.6"
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "plugin:tailwindcss/recommended"
    ],
    "rules": {
      "semi": "off",
      "@typescript-eslint/semi": [
        "error",
        "always"
      ],
      "no-console": "warn"
    }
  }
}
